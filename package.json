{
	"name": "rn-design-system-practice-mono",
	"version": "0.1.1",
	"description": "Practice React Native Design System with multiple component libraries",
	"private": true,
	"repository": {
		"type": "github",
		"url": "https://github.com/Retrojb/rn-ds-practice"
	},
	"installConfigs": {
		"hoistingLimits": "workspaces"
	},
	"workspaces": [
		"apps/*",
		"apps/storybook-mobile/",
		"packages/*",
		"packages/@teatez/*",
		"packages/@teatez-design-system/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"cleaner": "yarn workspace utils run clear-packages",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@turbo/gen": "^1.9.7",
		"eslint": "^7.32.0",
		"eslint-config-custom": "workspace:*",
		"prettier": "^2.5.1",
		"react-native-builder-bob": "^0.20.4",
		"turbo": "^1.9.7",
		"webpack-cli": "^5.1.1"
	},
	"dependencies": {
		"@babel/core": "^7.21.8",
		"@babel/preset-env": "^7.21.5",
		"@types/node": "^20.2.3",
		"audit-ci": "^6.6.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-native": "^0.71.8",
		"react-native-web": "^0.19.4",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4",
		"webpack": "^5.84.0"
	},
	"packageManager": "yarn@3.5.1",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"peerDependencies": {
		"react": ">=17.0.0",
		"react-native": ">=0.71.3",
		"react-native-web": "*"
	},
	"engines": {
		"node": ">= 18.0.0"
	}
}
